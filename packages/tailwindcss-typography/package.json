{
  "name": "@casoon/tailwindcss-typography",
  "version": "0.8.5",
  "description": "Content-specific typography presets for Tailwind CSS v4 - Blog prose, documentation, marketing copy, and reading optimizations",
  "keywords": [
    "tailwindcss",
    "css",
    "typography",
    "prose",
    "blog",
    "documentation",
    "content",
    "reading",
    "presets"
  ],
  "homepage": "https://github.com/jseidel/casoon-tailwind-effects#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jseidel/casoon-tailwind-effects.git"
  },
  "bugs": {
    "url": "https://github.com/jseidel/casoon-tailwind-effects/issues"
  },
  "author": "Julian Seidel <info@casoon.com> (https://casoon.com)",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist.css",
    "./dist.css": "./dist.css",
    "./dist.min.css": "./dist.min.css"
  },
  "main": "./dist.css",
  "files": [
    "dist.css",
    "dist.min.css",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npx tailwindcss -i ./src/index.css -o ./dist.css --minify",
    "validate": "node ../../scripts/validate-css.mjs",
    "prepublishOnly": "npm run build && npm run validate",
    "minify": "node ../../scripts/minify-css.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "tailwindcss": ">=4.0.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.13"
  }
}
