{
  "name": "@casoon/tailwindcss-effects",
  "version": "0.9.6",
  "description": "Complete collection of animation and UI effects for Tailwind CSS v4 - Consolidated v0.9.0 with 300+ utilities",
  "type": "module",
  "exports": {
    ".": "./index.css",
    "./index.css": "./index.css",
    "./dist.css": "./dist.css",
    "./dist.min.css": "./dist.min.css"
  },
  "main": "./index.js",
  "module": "./index.js",
  "style": "./dist.css",
  "sideEffects": [
    "./index.js",
    "./dist.css",
    "./dist.min.css"
  ],
  "scripts": {
    "minify": "node ../../scripts/minify-css.mjs",
    "validate": "node ../../scripts/validate-css.mjs",
    "prepublishOnly": "echo 'Building via root build system'"
  },
  "files": [
    "src/",
    "dist.css",
    "dist.min.css",
    "dist.core.css",
    "dist.core.min.css",
    "index.css",
    "core.css",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "dependencies": {
    "@casoon/tailwindcss-utilities": "^0.9.6",
    "@casoon/tailwindcss-animations": "^0.9.6",
    "@casoon/tailwindcss-glass": "^0.9.6",
    "@casoon/tailwindcss-orbs": "^0.9.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1"
  }
}
