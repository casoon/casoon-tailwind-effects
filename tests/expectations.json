{
  "version": "0.5.8",
  "packages": {
    "tailwindcss-animations": {
      "classes": [
        ".anim", ".fade-in", ".fade-out", ".scale-in", ".scale-out",
        ".slide-up", ".slide-down", ".slide-left", ".slide-right",
        ".blur-in", ".blur-out", ".rotate-in", ".rotate",
        ".pulse", ".bounce", ".wiggle",
        ".hover-lift-sm", ".hover-lift-md", ".hover-scale-105", ".hover-scale-110",
        ".anim-xxs", ".anim-xs", ".anim-sm", ".anim-md", ".anim-lg", ".anim-xl", ".anim-2xl",
        ".ease-standard", ".ease-emphasized", ".ease-decelerate", ".ease-accelerate",
        ".ease-spring", ".ease-soft-spring"
      ],
      "cssVariables": [
        "--cs-anim-duration-xxs", "--cs-anim-duration-xs", "--cs-anim-duration-sm",
        "--cs-anim-duration-md", "--cs-anim-duration-lg", "--cs-anim-duration-xl", "--cs-anim-duration-2xl",
        "--cs-anim-ease-standard", "--cs-anim-ease-emphasized", "--cs-anim-ease-decelerate",
        "--cs-anim-ease-accelerate", "--cs-anim-ease-spring", "--cs-anim-ease-soft-spring",
        "--cs-anim-shadow-ink", "--cs-anim-reduced-motion-duration", "--cs-anim-reduced-motion-ease"
      ]
    },
    "tailwindcss-utilities": {
      "classes": [
        ".sr-only", ".container-fluid"
      ],
      "cssVariables": []
    },
    "tailwindcss-glass": {
      "classes": [
        ".glass", ".glass-card", ".glass-card-light", ".glass-nav", ".glass-nav-light", ".glass-button",
        ".glass-sm", ".glass-lg", ".glass-weak", ".glass-medium", ".glass-strong",
        ".glass-blue", ".glass-purple", ".glass-green", ".glass-pink", ".glass-amber",
        ".glass-rounded", ".glass-shadow", ".glass-tooltip"
      ],
      "cssVariables": [
        "--cs-glass-white", "--cs-glass-black", "--cs-glass-blue", "--cs-glass-purple",
        "--cs-glass-green", "--cs-glass-pink", "--cs-glass-amber",
        "--cs-glass-bg-light", "--cs-glass-bg-medium", "--cs-glass-bg-strong",
        "--cs-glass-border-light", "--cs-glass-border-medium", "--cs-glass-border-strong",
        "--cs-glass-shadow-light", "--cs-glass-shadow-medium", "--cs-glass-shadow-strong"
      ]
    },
    "tailwindcss-loading": {
      "classes": [
        ".spinner", ".loading"
      ],
      "cssVariables": [
        "--cs-loading-spinner-base", "--cs-loading-spinner-active"
      ]
    },
    "tailwindcss-navigation": {
      "classes": [
        ".nav", ".nav-item", ".nav-link", ".nav-pills", ".nav-tabs", ".navbar-brand", ".navbar-nav"
      ],
      "cssVariables": [
        "--cs-nav-primary", "--cs-nav-white", "--cs-nav-text-muted", "--cs-nav-border"
      ]
    },
    "tailwindcss-scroll": {
      "classes": [
        ".scrollbar-none", ".scrollbar-thin", ".scrollbar-auto",
        ".scrollbar-thumb-gray", ".scrollbar-thumb-blue", ".scrollbar-track-transparent",
        ".scroll-smooth", ".scroll-auto", ".snap-start", ".snap-center", ".snap-end"
      ],
      "cssVariables": [
        "--cs-scroll-thumb-gray", "--cs-scroll-thumb-blue"
      ]
    },
    "tailwindcss-gradients": {
      "classes": [
        ".gradient-blue", ".gradient-purple", ".gradient-green", ".gradient-pink", ".gradient-orange",
        ".gradient-fire", ".gradient-ocean", ".gradient-sunset", ".gradient-mint",
        ".gradient-text", ".gradient-border", ".gradient-radial-center", ".gradient-conic"
      ],
      "cssVariables": [
        "--cs-gradient-blue-start", "--cs-gradient-blue-end",
        "--cs-gradient-purple-start", "--cs-gradient-purple-end",
        "--cs-gradient-fire-start", "--cs-gradient-fire-end",
        "--cs-gradient-ocean-start", "--cs-gradient-ocean-end",
        "--cs-gradient-sunset-start", "--cs-gradient-sunset-mid", "--cs-gradient-sunset-end"
      ]
    },
    "tailwindcss-orbs": {
      "classes": [
        ".orb", ".orb-xs", ".orb-sm", ".orb-md", ".orb-lg", ".orb-xl", ".orb-2xl",
        ".orb-gradient-blue", ".orb-gradient-purple", ".orb-gradient-pink",
        ".orb-float", ".orb-pulse", ".orb-drift", ".orb-blur-sm", ".orb-blur-md", ".orb-blur-lg"
      ],
      "cssVariables": [
        "--cs-orb-blue", "--cs-orb-purple", "--cs-orb-pink",
        "--cs-orb-gradient-blue", "--cs-orb-gradient-purple", "--cs-orb-gradient-pink"
      ]
    },
    "tailwindcss-micro-interactions": {
      "classes": [
        ".interactive", ".smooth-interaction", ".hover-scale-105", ".hover-scale-110",
        ".focus-ring-2", ".focus-ring-4", ".active-scale-95", ".tap-scale-95",
        ".hover-brighten", ".hover-darken", ".hover-rotate-1", ".hover-rotate-3"
      ],
      "cssVariables": [
        "--cs-micro-focus-ring", "--cs-micro-shadow", "--cs-micro-shadow-light"
      ]
    }
  }
}
